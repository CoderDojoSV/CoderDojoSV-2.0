                                                   function(){
                                                       fields[i] = this;
                                                       i++;
                                                   });
                                               $(':hidden', this).each(
                                                   function(){
                                                       var bday = false;
                                                       if (fields.length == 2){
                                                           bday = true;
                                                           fields[2] = {'value':1970};//trick birthdays into having years
                                                       }
